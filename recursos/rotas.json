    "info": {
        "title": "Koa Workflow",
        "description": "Routes for accessing workflow methods",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:3000"
        }
    ],
    "tags": [
        {
            "name": "Token",
            "description": "Token for authentication and authorization"
        },
        {
            "name": "Workflow"
        },
        {
            "name": "Process"
        },
        {
            "name": "Activities"
        },
